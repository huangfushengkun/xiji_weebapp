a,span{
    font-size: .24rem;
}
h2{
    font-size: .30rem;
}
header.top-nav{
    width: 100%;
    height: 1.94rem;
    position: relative;
    .home-nav{
        width: 100%;
        height: 1.04rem;
        // background-color: purple;
        .logo{
            display: block;
            position: absolute;
            top: .34rem;
            left: 50%;
            display: flex;
            margin-left: -1.4rem;
            height: .46rem;
            width: 2.8rem;
            overflow: hidden;
            text-align: center;
            vertical-align: middle;
            img{
                height: 100%;
                width: 100%;
            }
        }
        .item{
            line-height: 0.46rem;
            color: #ffffff;
            text-align: center;
            outline: none;
            padding: 0;
            // text-indent: -9999px;
            display: block;
            position: absolute;
            top: .34rem;
            height: .46rem;
            width: .46rem;
        }
        .lang-setting{
            left: .20rem;
            background: url(../images/index/topbar/set_icon_2x.png);
            background-size: .46rem .46rem;
        }
        .user-right{
            right: 0.96rem;
            background: url(../images/index/topbar/header_icon_user_2x_160701.png);
            background-size: .46rem .46rem;
        }
        .cart-right{
            right: 0.2rem;
            background: url(../images/index/topbar/header_icon_cart_2x_160701.png);
            background-size: .46rem .46rem;
        }
    }
    //搜索框样式
    #index_sratch_block{
        position: absolute;
        left: 0;
        bottom: 0;
        width: 100%;
        height: 0.9rem;
        form{
            padding: 0 .20rem .14rem;
            .search{
                position: relative;
                height: .76rem;
                background-color: #fafafa;
                border: 1px solid #E4E4E4;
                margin: 0 auto;
                border-radius: 3px;
                display: flex;
                input{
                    display: block;
                    font-size: .22rem;
                    font-family: inherit;
                    border: none;
                    width: 98%;
                    padding: 0 0 0 2%;
                    margin-right: 2%;
                    height: .72rem;
                    line-height: .72rem;
                    border-radius: .03rem;
                    background: url(../images/index/topbar/search_icon01_2x_160701.png) right .16rem no-repeat ;
                    background-size: .44rem .44rem;

                }
            }
        }
    }
}
/* 顶部搜索区域样式结束 */

/* 焦点图样式开始 */
.xiji_banner{
    width: 100%;
    height: 4rem;
    img{
        width: 100%;
    }
}
/* 焦点图样式结束 */

/* 导航样式开始 */
.xiji-nav{
    width: 100%;
    height: 3rem;
    padding: .4rem .2rem 0 .2rem;
    margin-top: -0.2rem;
    background: url(../images/index/banner/5201.png) left bottom / 100% 100% no-repeat;
    overflow: hidden;
    box-sizing: border-box;
    background-color: #fff;
    margin-bottom: 0;
    position: relative;
    z-index: 2;
    border-top-left-radius: .24rem;
    border-top-right-radius: .24rem;
    border-bottom-right-radius: 0px;
    border-bottom-left-radius: 0px;
    display: flex;
    flex-wrap: wrap;
    .index-advert-wap-inner{
        width: 33.3%;
        height: 0.88rem;
        font-size: .22rem;
        text-align: center;
        p{
            height: auto;
            color: #fcdec2;
        }
        img{
            width: 0.48rem;
            height: 0.48rem;
            vertical-align: bottom;
            margin-bottom: .04rem;
            border-radius: .36rem;
        }
    }
}
/* 导航样式结束 */
.xj-horizontal-box{
    height: 2.4rem;
    a{
        img{
            display: block;
            width: 100%;
            height: 2.4rem;
            vertical-align: top;
        }
    }
    
}

/* 新用户专区样式 */
.xj-newuser-box{
    height: 7.92rem;
    .newuser-top, .newuser-content{
        height: 1.89rem;
    }
    .newuser-botttom{
        margin-top: -0.02rem;
        div{
            float: left;
        }
        height: 4.10312rem;
        .newuser-botttom-left{
            width: 3.2025rem;
            height: 4.10312rem;
        }
        .newuser-botttom-right{
            width: 4.2975rem;
            height: 4.19rem;
            .newuser-botttom-right-top{
                width: 100%;
                height: 2.02rem;
                div{
                    width: 50%;
                }
            }
            .newuser-botttom-right-bot{
                margin-top: -0.02rem;
                width: 100%;
                height: 50%;
            }
        }
    }
    
}
/* 新用户专区样式结束 */

/* 每日底价样式 */
.xj-limitprice-box{
    margin: .20rem .20rem 0;
    height: 5.27532rem;
    width: 7.1rem;
    .heading-block{
        height: 0.96rem;
        line-height: 0.96rem;
        padding: 0 .20rem;
        border: none;
        text-align: left;
        background:url(../images/index/main/limitprice_bg02.png) center left no-repeat;
        background-size: cover;
        display: flex;
        justify-content: space-between;
        h2{

            width: 1.8rem;
            color: #ffffff;
            font-size: 0.3rem;
            font-weight: normal;
        }
        a{
            width: 1.22rem;
            height: 0.96rem;
            color: #ffffff;
            font-size: 0.24rem;
            padding-right: 0.26rem;
            background: url(../images/index/main/limitprice_icon01.png) center right no-repeat;
            background-size: 0.12rem 0.16rem;
        }
    }
    .daily_special{
        height: 4.31532rem;
        ul{
            display: flex;
            flex-wrap: wrap;
            li{
                width: 50%;
                height: 1.82rem;
                a{
                    display: flex;
                    padding: 0.2rem;
                    width: 100%;
                    height: 1.8rem;
                    margin-bottom: .02rem;
                    background-color: #fff;
                    .goods-img{
                        width: 1.4rem;
                        height: 1.4rem;
                    }
                    .goods-info{
                        width: 1.55rem;
                        height: 1.4rem;
                        min-height: 70px;
                        margin-left: 0.2rem;
                        .goods-name{
                            width: 100%;
                            height: 50%;
                            overflow: hidden;
                            color: #666;
                            font-size: 12px;
                            line-height: 1.5;
                            text-align: left;
                        }
                        .pri-box{
                            height: 50%;
                            .goods-price{
                                color: #ed5831;
                                font-size: 14px;
                                font-weight: bold;
                                line-height: 1;
                                height: 0.28rem;
                            }
                            .goods-delete{
                                height: 0.24rem;
                                line-height: 1;
                                color: #999999;
                                font-size: 0.2rem;
                            }
                        }
                    }

                }
            }
        }
    }
    /* 倒计时 */
    .time-countdown{
        padding: .14rem 0;
        height: 0.6754rem;
        background-color: #fff;
        display: flex;
        justify-content: center;
        margin-top: -0.50rem;
        span{
            display: inline-block;
            vertical-align: middle;
        }
        .ttl-box{
            width: 1.92rem;
            color: #333333;
            font-size: 0.2rem;
            height: 0.36rem;
            line-height: 0.4rem;
            margin-right: 0.4rem;
        }
        .time-box{
            width: 1.98rem;
            height: 18px;
            color: #fff;
            font-size: 0.2rem;
            i{
                position: relative;
                display: inline-block;
                vertical-align: middle;
                width: 18px;
                height: 18px;
                line-height: 20px;
                margin-right: 15px;
                text-align: center;
                background-color: #333333;
                border-radius: 50%;
            }
            i:nth-child(2):after {
                display: inline-block;
                height: 18px;
                content: ':';
                color: #999;
                font-size: 10px;
                position: absolute;
                left: 23px;
                top: 0;
            }
            i:nth-child(1):after {
                display: inline-block;
                height: 18px;
                content: ':';
                color: #999;
                font-size: 10px;
                position: absolute;
                left: 23px;
                top: 0;
            }
        }
    }
}
/* 每日底价样式结束 */
/* 春假发货安排通知样式 */
.xj-notices-box {
    box-sizing: border-box;
    padding: .20rem;
    height: 0.7rem;
    background-color: #fff;
    border-bottom: 1px solid #e2e2e2;
    font-size: 0.24rem;
    display: flex;

    .notices-tit{
        display: inline-block;
        width: 0.3rem;
        height: 0.3rem;
        background: url(../images/index/main/tongzhi.png); 
        background-size: 100% 100%;
        
    }
    .notices-con{
        line-height: 0.28rem;
        a{
            color: #333333;
            margin-left: 0.2rem;
        }
    }
}
/* 春假发货安排通知样式结束 */

