		}
		return null;
	};

	//返回某键值为指定值的
	HotMakeProds.makeProductList = function(){

		var fixedIsArray = $.isArray(this.fixed_product_list),
			randomIsArray = $.isArray(this.random_product_list),
			randomIdx = 0,
			fetchFixedProd = null,
			randomIdxList = [];//存储random_product_list中被打乱的下标，通过从头读取打乱的下标而随机取得数组元素。

		if(randomIsArray){
			randomIdxList = $.map(this.random_product_list, function(el, i){ return i; });
			//randomIdxList.sort(function(a,b){return Math.random()>0.5?1:0}); //简单打乱方法
            //深度打乱
            for(var i= 0,l = randomIdxList.length;i<l;i++){
                var n = parseInt(Math.random()*l);
                randomIdxList[i] = [randomIdxList[n],randomIdxList[n] = randomIdxList[i]][0];//执行随机位置调换
            }
        }

		for(var i=1;i<=6;i++) {
			if(fixedIsArray) fetchFixedProd = this.getKeyData(this.fixed_product_list,'order',i);
			if(fetchFixedProd!= null) { 
				this.maked_product_list.push(fetchFixedProd);
			}else{
				if (randomIsArray && randomIdx<randomIdxList.length){
					this.maked_product_list.push(this.random_product_list[ randomIdxList[randomIdx] ]);
					this.maked_product_list[this.maked_product_list.length-1].order = i;
				} 
				randomIdx++;
			}
		}


	}

