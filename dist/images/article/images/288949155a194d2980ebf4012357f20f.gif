    //显示联查列表
    $("input[name='search_keywords']").on({
        keyup: function() {
            search_associate();
        },
        touchend: function() {
            search_associate();
        },
        change: function() {
            search_associate();
        },
        input: function() {
            search_associate();
        },
        propertychange: function() {
            search_associate();
        },
    });

    function search_associate(){
        $("#clearInput").css("display","block");

        if($("input[name='search_keywords']").val() != ""){
            $.ajax({
                url:'/wap/search-associate.html',
                type:'post',
                data:'value='+$("input[name='search_keywords']").val(),
                //async:false, //默认为true 异步
                dataType: "json",
                error:function(){
                    // alert('error');
                },
                success:function(data){
                    if(data.words){
                        $("#searchBox_list ul").html("");
                        $.each (data.words, function(key,val){
                            $("#searchBox_list ul").append("<li><a href='/wap/gallery-index_new.html?scontent=n,"+val+"'><span class='association-product-name'>"+val+"</span></a></li>");
                        });
                    }
                }
            });
            $("#searchBox_list").css("display","block");
        }else{
            removeSearchList();
        }
    }

    //清除input值
    $("#clearInput").click(function(){
    	$("input[name='search_keywords']").val("");
    	removeSearchList();
    });
    
    function removeSearchList(){
    	$("#clearInput").css("display","none");
        if($("#searchBox_list").length>0){
            $("#searchBox_list").css("display","none");
            $("#searchBox_list ul").html("");
        }
    }
</script>        </div>
